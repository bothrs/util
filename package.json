{
  "name": "@bothrs/util",
  "version": "1.9.0",
  "description": "Common helper functions",
  "license": "MIT",
  "scripts": {
    "build": "yarn lint",
    "dev": "yarn lint",
    "fix": "prettier --write -l *.mjs",
    "lint": "prettier -l *.mjs",
    "prepare": "yarn fix",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "*.js",
    "*.mjs",
    "*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebothrs/util.git"
  },
  "contributors": [
    "Thomas Ghysels <info@thomasg.be> (https://thomasg.be)"
  ],
  "bugs": {
    "url": "https://github.com/thebothrs/util/issues"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": false,
    "parser": "babel",
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "homepage": "https://github.com/thebothrs/util#readme"
}
