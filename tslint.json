{
  "extends": ["tslint:latest", "tslint-react"],
  "linterOptions": {
    "project": "./tsconfig.json",
    "exclude": []
  },
  "rules": {
    "no-console": [false],
    "no-empty": false,
    "no-shadowed-variable": false,
    "no-submodule-imports": false,
    "no-unused-variable": [true],
    "whitespace": false
  },
  "jsRules": true
}